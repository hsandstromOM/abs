  "description": "abs rebuild",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "webpack --watch & nodemon server.js",
    "build": "watchify browser.js -o www/browser.js & node server.js & npm run watch-css",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-css": "node-sass --include-path scss scss/main.scss www/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "build:dev": "browserify browser.js | uglifyjs -cm | tee www/browser.min.js",
    "prod": "browserify browser.js | uglifyjs -cm > www/browser.min.js"
  },
